dsLinking <- subset(Links79Pair, RelationshipPath=="Gen2Siblings")
dsOutcomes <- Gen2AFIConcor3
dsOutcomes$SubjectTag <-dsOutcomes$CASEID
dsD <- CreatePairLinksDoubleEntered(
outcomeDataset = dsOutcomes,
linksPairDataset = dsLinking,
outcomeNames = c("Intel_G1","AFI_G2"))
dsD$DIF_G<-#abs
(dsD$Intel_G1_S1-dsD$Intel_G1_S2)
dsD$DIF_A<-#abs
(dsD$AFI_G2_S1-dsD$AFI_G2_S2)
max_val <- max(abs(dsD$DIF_G),na.rm=TRUE)
values <- seq(-max_val, max_val, length = length(shading))
coef<-coef(lm(AFI_G1 ~ Intel_G1, data = dsOutcomes))
max_val <- max(abs(dsD$DIF_G),na.rm=TRUE)
values <- seq(-max_val, max_val, length = length(shading))
g1<- ggplot(dsD, aes(y=AFI_G2_S1, x=Intel_G1_S1,colour=(DIF_G))) +
geom_point(size =.5,alpha = 1,na.rm=TRUE,position = position_jitter(w = 0.015, h = .75)) + geom_abline(intercept = coef[1], slope = coef[2])+
scale_colour_gradientn(name="Sibling \nDifferences \nin Ability",colours=shading,values=values,rescaler = function(x, ...) x, oob = identity)+
theme_min(size=8) +
theme(plot.title=element_text(size=8),
plot.margin=unit(c(0,0.0,0,0), "cm"),
legend.position= "left",
legend.key.width=unit(.5,"cm"))+
ylab("Gen1 Age at First Intercourse") +
xlab("Gen1 AFQT (Age 14-21)") + coord_cartesian(ylim=c(12,30))+
theme(text = element_text(size=8))#+
