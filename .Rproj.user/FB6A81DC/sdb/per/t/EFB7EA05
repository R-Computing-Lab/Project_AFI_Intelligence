f <- coxph(Surv(TimeM, EventM) ~ O + C + E + A + N + Otest + Ctest + Etest + Atest + Ntest)\n\ndetach(CorSPnamed)\n\ndetach(Selfnamed)\n#######Mate\n\nattach(Matenamed)\n\nModel_Mean_Mate_est <- coxph(Surv(TimeM, EventM) ~ O + C + E + A + N)\n\nattach(Matedif)\nModel_Diff_Mate_est <- coxph(Surv(TimeM, EventM) ~ O + C + E + A + N + Otest + Ctest + Etest + Atest + Ntest)\ndetach(Matedif)\n\nattach(CorPPnamed)\n\nModel_CorPP_Mate_est <- coxph(Surv(TimeM, EventM) ~ O + C + E + A + N + Otest + Ctest + Etest + Atest + Ntest)\ndetach(CorPPnamed)\nattach(CorSSnamed)\n\nModel_CorSS_Mate_est <- coxph(Surv(TimeM, EventM) ~ O + C + E + A + N + Otest + Ctest + Etest + Atest + Ntest)\ndetach(CorSSnamed)\n\nattach(CorMPnamed)\n\nModel_CorMP_Mate_est <- coxph(Surv(TimeM, EventM) ~ O + C + E + A + N + Otest + Ctest + Etest + Atest + Ntest)\n\ndetach(CorMPnamed)\nattach(CorSPnamed)\nModel_CorMP_Mate_est <- coxph(Surv(TimeM, EventM) ~ O + C + E + A + N + Otest + Ctest + Etest + Atest + Ntest)\n\ndetach(CorSPnamed)\nattach(CorMMnamed)\n\nModel_CorMM_Mate_est <- coxph(Surv(TimeM, EventM) ~ O + C + E + A + N + Otest + Ctest + Etest + Atest + Ntest)\ndetach(CorMMnamed)\ndetach(Matenamed)\n@\n\n<<table_coxcox_peer_model,echo=FALSE, comment=NA, include=FALSE>>=\nlibrary(stargazer)\nz<-capture.output(stargazer(Model_Mean_Peer,Model_Diff_Peer, Model_CorPP_Peer, Model_CorMP_Peer, float = FALSE,dep.var.labels=c(\"Time until Divorce\"),digits=3\n                            , covariate.labels=c(\"Openness\",\"Conscientiousness\", \"Extraversion\",\"Agreeableness\",\"Neuroticism\",\"Openness\",\"Conscientiousness\", \"Extraversion\",\"Agreeableness\",\"Neuroticism\")),file=paste(\"table_cox_peer_model\",\".tex\", sep=\"\"))\n@\n\\begin{longtable}{@{\\extracolsep{5pt}}lcccc} \n\\caption{Peer Models}\\label{table_cox_peer_model}\n\\partialinput{5}{10}{table_cox_peer_model}\n & Main Effect & Gender Effect & Peer-Rated Similarity & Mate-Peer Rater Agreement \\\\\n \\hline \\\\[-1.8ex] \n\\partialinput